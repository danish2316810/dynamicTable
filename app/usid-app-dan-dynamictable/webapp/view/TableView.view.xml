<mvc:View controllerName="usib.app.dan.usidappdandynamictable.controller.TableView"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Page id="page" title="{i18n>title}">
         <content>
            
            <VBox >
                <Select id="errorTypeSelect"
                change=".onErrorTypeChange"
                items="{
                  path: 'JasonModel>/errorTypes'
                }">
          <core:Item key="{JasonModel>key}" text="{JasonModel>text}" xmlns:core="sap.ui.core"/>
        </Select>
        <!-- Filter bar -->
        <Toolbar id="filterBar" design="Auto" />
          <Table id="dynamicTable" inset="false" mode="SingleSelectMaster" selectionChange="onLineItemClick">
            <headerToolbar>
                <Toolbar>
                <Title text="Error Data"/>
                </Toolbar>
            </headerToolbar>
            </Table>
            </VBox>
     
         </content>
    </Page>
</mvc:View>